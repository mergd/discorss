{
    "name": "discorss-bot",
    "version": "1.0.0",
    "author": "mergd",
    "description": "A discord.js bot for RSS feeds",
    "license": "MIT",
    "private": true,
    "engines": {
        "bun": ">=1.0.0"
    },
    "type": "module",
    "exports": [
        "./dist/start-bot.js",
        "./dist/start-manager.js"
    ],
    "scripts": {
        "lint": "bunx eslint . --cache --ext .js,.jsx,.ts,.tsx",
        "lint:fix": "bunx eslint . --fix --cache --ext .js,.jsx,.ts,.tsx",
        "format": "bunx prettier --check .",
        "format:fix": "bunx prettier --write .",
        "clean": "git clean -xdf --exclude=\"./config/**/*\"",
        "clean:dry": "git clean -xdf --exclude=\"./config/**/*\" --dry-run",
        "build": "bun run tsc --project tsconfig.json",
        "commands:view": "bun run build && bun --enable-source-maps dist/start-bot.js commands view",
        "commands:register": "bun run build && bun --enable-source-maps dist/start-bot.js commands register",
        "commands:rename": "bun run build && bun --enable-source-maps dist/start-bot.js commands rename",
        "commands:delete": "bun run build && bun --enable-source-maps dist/start-bot.js commands delete",
        "commands:clear": "bun run build && bun --enable-source-maps dist/start-bot.js commands clear",
        "start": "bun run start:manager",
        "start:bot": "bun run build && bun --enable-source-maps dist/start-bot.js",
        "start:manager": "bun run build && bun --enable-source-maps dist/start-manager.js",
        "test": "bun test",
        "test:watch": "bun test --watch",
        "test:coverage": "bun test --coverage",
        "db:generate": "bunx drizzle-kit generate --config=src/drizzle.config.ts",
        "db:push": "bunx drizzle-kit push --config=src/drizzle.config.ts",
        "db:studio": "bunx drizzle-kit studio",
        "db:migrate": "bunx drizzle-kit migrate --config=src/drizzle.config.ts",
        "db:backup": "bun run build && bun --enable-source-maps dist/scripts/backup-db.js",
        "db:restore": "bun run build && bun --enable-source-maps dist/scripts/restore-db.js",
        "docker:build": "docker build -t discord-bot-rss . && docker run -d --name discord-bot-rss discord-bot-rss",
        "docker:run": "docker start discord-bot-rss"
    },
    "dependencies": {
        "@discordjs/rest": "^2.2.0",
        "@libsql/client": "^0.15.3",
        "@openrouter/ai-sdk-provider": "^0.4.5",
        "@types/pg": "^8.11.11",
        "ai": "^4.3.7",
        "class-transformer": "0.5.1",
        "class-validator": "0.14.1",
        "cron-parser": "^4.9.0",
        "date-fns": "^4.1.0",
        "discord-hybrid-sharding": "^2.2.6",
        "discord.js": "^14.15.2",
        "discord.js-rate-limiter": "1.3.2",
        "dotenv": "^16.4.5",
        "drizzle-orm": "^0.44.5",
        "express": "4.21.2",
        "express-promise-router": "4.1.1",
        "filesize": "10.1.6",
        "jsdom": "^26.1.0",
        "linguini": "1.3.1",
        "luxon": "3.5.0",
        "node-fetch": "3.3.2",
        "node-schedule": "2.1.1",
        "pg": "^8.14.1",
        "pino": "9.6.0",
        "pino-pretty": "13.0.0",
        "postgres": "^3.4.5",
        "posthog-node": "^4.14.0",
        "reflect-metadata": "^0.2.2",
        "remove-markdown": "0.6.0",
        "rss-parser": "^3.13.0",
        "tslib": "^2.6.2",
        "typescript": "^5.8.3",
        "uuid": "^10.0.0"
    },
    "devDependencies": {
        "@types/express": "4.17.21",
        "@types/jsdom": "^21.1.7",
        "@types/luxon": "3.4.2",
        "@types/node": "^20.12.12",
        "@types/node-schedule": "2.1.7",
        "@types/remove-markdown": "0.3.4",
        "@types/uuid": "^9.0.8",
        "@typescript-eslint/eslint-plugin": "^8.24.1",
        "@typescript-eslint/parser": "^8.24.1",
        "drizzle-kit": "^0.31.4",
        "eslint": "^9.20.1",
        "eslint-plugin-import": "^2.31.0",
        "eslint-plugin-unicorn": "^57.0.0",
        "prettier": "^3.5.1"
    }
}
